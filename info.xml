<?xml version="1.0"?>
<extension key="bankingdemo" type="module">
  <file>bankingdemo</file>
  <name>CiviBanking Demo</name>
  <description>Gives a little CiviBanking tour and demonstration</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>B. Endres</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
